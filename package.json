{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "setup": "npm install && npm run dev",
    "quickstart": "npm install && npm run dev",
    "test": "jest --maxWorkers=100%",
    "test:watch": "jest --watch --maxWorkers=100%",
    "test:fast": "jest --maxWorkers=100% --verbose=false",
    "test:coverage": "jest --coverage --maxWorkers=100%",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=100%",
    "test:all": "jest --verbose --coverage --maxWorkers=100%",
    "test:unit": "jest --testPathPatterns='__tests__/(lib|components|utils)' --verbose --coverage --maxWorkers=100%",
    "test:unit-fast": "jest --testPathPatterns='__tests__/(lib|components|utils)' --maxWorkers=100%",
    "test:integration": "jest --testPathPatterns='__tests__/integration' --verbose --coverage --maxWorkers=100%",
    "test:behavioral": "jest --testPathPatterns='__tests__/behavioral' --verbose --coverage --maxWorkers=100%",
    "test:features": "jest --testPathPatterns='__tests__/features' --verbose --coverage --maxWorkers=100%",
    "test:themes": "jest --testNamePattern='theme|Theme' --verbose --coverage --maxWorkers=100%",
    "test:pages": "jest --testPathPatterns='__tests__/app' --verbose --coverage --maxWorkers=100%",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:pages && npm run test:themes",
    "test:coverage-check": "jest --coverage --passWithNoTests --maxWorkers=100%",
    "test:coverage-report": "jest --coverage --coverageReporters=text-lcov --maxWorkers=100% | coveralls",
    "test:coverage-80": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}' --maxWorkers=100%",
    "test:coverage-90": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":90,\"functions\":90,\"lines\":90,\"statements\":90}}' --maxWorkers=100%",
    "test:coverage-95": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":95,\"functions\":95,\"lines\":95,\"statements\":95}}' --maxWorkers=100%",
    "test:coverage-98": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":98,\"functions\":98,\"lines\":98,\"statements\":98}}' --maxWorkers=100%",
    "test:coverage-current": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":0,\"functions\":0,\"lines\":0,\"statements\":0}}' --maxWorkers=100%",
    "test:pending": "jest --coverage --watchAll=false --maxWorkers=100%"
  },
